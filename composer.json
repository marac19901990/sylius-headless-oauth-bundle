{
    "name": "marac19901990/sylius-headless-oauth-bundle",
    "description": "Headless OAuth authentication for Sylius via API Platform (Google, Apple)",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "sylius",
        "oauth",
        "google",
        "apple",
        "headless",
        "api-platform",
        "authentication"
    ],
    "authors": [
        {
            "name": "Marko Petrovic",
            "email": "marac19901990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "api-platform/core": "^3.0",
        "lexik/jwt-authentication-bundle": "^2.0|^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.14|^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.0",
        "sylius/core-bundle": "^1.12|^2.0",
        "symfony/phpunit-bridge": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Marac\\SyliusHeadlessOAuthBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marac\\SyliusHeadlessOAuthBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
